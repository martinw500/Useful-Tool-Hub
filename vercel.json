{
  "version": 2,
  "builds": [
    {
      "src": "instagram-downloader/api/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "youtube-downloader/api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/instagram(.*)",
      "dest": "instagram-downloader/api/index.py",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, POST, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type"
      }
    },
    {
      "src": "/api/youtube(.*)",
      "dest": "youtube-downloader/api/index.py",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, POST, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type"
      }
    }
  ]
}
